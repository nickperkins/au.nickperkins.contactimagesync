<?xml version="1.0"?>
<extension key="au.nickperkins.contactimagesync" type="module">
  <file>contactimagesync</file>
  <name>au.nickperkins.contactimagesync</name>
  <description>Extension to sync CiviCRM contact images CMS user profiles.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Nick Perkins</author>
    <email>nick@nickperkins.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/nickperkins/au.nickperkins.contactimagesync</url>
    <url desc="Documentation">https://github.com/nickperkins/au.nickperkins.contactimagesync/wiki</url>
    <url desc="Support">https://github.com/nickperkins/au.nickperkins.contactimagesync/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-01-21</releaseDate>
  <version>1.3.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Contactimagesync</namespace>
    <format>23.02.1</format>
    <angularModule>crmContactimagesync</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <upgrader>CRM_Contactimagesync_Upgrader</upgrader>
</extension>
